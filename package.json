{
	"name": "4dn-microscopy-metadata-tool",
	"version": "0.0.21",
	"main": "./dist/MicroscopyMetadataTool.min.js",
	"unpkg": "./dist/MicroscopyMetadataTool.min.js",
	"module": "./es/app.js",
	"private": true,
	"scripts": {
		"test": "node scripts/test.js",
		"buildProd": "gulp build-prod",
		"buildDev": "gulp build-dev",
		"dev": "gulp dev",
		"build": "gulp build-prod && gulp build-dev",
		"build-all": "npm run build-es && gulp build",
		"build-es": "npx babel ./src --out-dir ./es --env-name esm --verbose",
		"watch-es": "npx babel ./src --out-dir ./es --env-name esm --verbose --watch"
	},
	"publishConfig": {
		"access": "restricted"
	},
	"dependencies": {
		"bfj": "6.1.1",
		"file-loader": "3.0.1",
		"html2canvas": "^1.0.0-rc.3",
		"identity-obj-proxy": "3.0.0",
		"imagemin": "^6.1.0",
		"img-loader": "^3.0.1",
		"jquery": "^3.4.1",
		"jsonschema": "^1.2.4",
		"popper.js": "^1.15.0",
		"rc-tabs": "^9.6.4",
		"react-app-polyfill": "^1.0.1",
		"react-bootstrap": "^1.0.0-beta.9",
		"react-collapsible": "^2.6.0",
		"react-drag-drop-container": "^6.1.1",
		"react-dropzone": "^10.1.5",
		"react-jsonschema-form": "^1.6.1",
		"react-resizable": "^1.9.0",
		"react-simple-animate": "^3.2.0",
		"resolve": "1.11.0",
		"url-loader": "1.1.2",
		"uuid": "^3.3.2"
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": [
		">0.2%",
		"not dead",
		"not ie <= 11",
		"not op_mini all"
	],
	"devDependencies": {
		"@babel/cli": "^7.6.2",
		"@babel/core": "^7.5.4",
		"@babel/plugin-proposal-class-properties": "^7.3.3",
		"@babel/plugin-proposal-object-rest-spread": "^7.3.2",
		"@babel/plugin-syntax-dynamic-import": "^7.2.0",
		"@babel/preset-env": "^7.1.0",
		"@babel/preset-react": "^7.0.0",
		"@babel/register": "^7.0.0",
		"babel-eslint": "^10.0.2",
		"babel-loader": "^8.0.6",
		"case-sensitive-paths-webpack-plugin": "2.2.0",
		"css-loader": "2.1.1",
		"eslint": "^6.6.0",
		"eslint-loader": "^2.1.2",
		"eslint-plugin-flowtype": "3.9.1",
		"eslint-plugin-import": "^2.17.2",
		"eslint-plugin-jsx-a11y": "^6.2.1",
		"eslint-plugin-react": "^7.13.0",
		"fancy-log": "^1.3.3",
		"gulp": "^4.0.2",
		"jest": "24.8.0",
		"jest-pnp-resolver": "1.2.1",
		"jest-resolve": "24.8.0",
		"jest-watch-typeahead": "^0.3.1",
		"plugin-error": "^1.0.1",
		"react": "^16.8.6",
		"react-dom": "^16.8.6",
		"source-map-support": "^0.5.11",
		"string-replace-loader": "^1.0.0",
		"style-loader": "^1.0.0",
		"terser-webpack-plugin": "1.3.0",
		"webpack": "^4.36.1",
		"webpack-cli": "^3.3.6"
	},
	"peerDependencies": {
		"react": ">=16.8.6",
		"react-dom": ">=16.8.6"
	},
	"jest": {
		"collectCoverageFrom": [
			"src/**/*.{js,jsx,ts,tsx}",
			"!src/**/*.d.ts"
		],
		"resolver": "jest-pnp-resolver",
		"setupFiles": [
			"react-app-polyfill/jsdom"
		],
		"testMatch": [
			"<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
			"<rootDir>/src/**/?(*.)(spec|test).{js,jsx,ts,tsx}"
		],
		"testEnvironment": "jsdom",
		"testURL": "http://localhost",
		"transform": {
			"^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
			"^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
			"^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
		},
		"transformIgnorePatterns": [
			"[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
			"^.+\\.module\\.(css|sass|scss)$"
		],
		"moduleNameMapper": {
			"^react-native$": "react-native-web",
			"^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
		},
		"moduleFileExtensions": [
			"web.js",
			"js",
			"web.ts",
			"ts",
			"web.tsx",
			"tsx",
			"json",
			"web.jsx",
			"jsx",
			"node"
		]
	}
}
